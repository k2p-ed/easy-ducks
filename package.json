{
  "name": "easy-ducks",
  "version": "0.1.0",
  "description": "A utility to simplify the Redux modular duck pattern",
  "author": "Knowledge to Practice <opensource@k2p.com> (http://www.knowledgetopractice.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/k2p-ed/easy-ducks"
  },
  "license": "MIT",
  "private": false,
  "main": "lib/easy-ducks.cjs.js",
  "module": "lib/easy-ducks.esm.js",
  "scripts": {
    "build": "rimraf lib && rollup -c && yarn copy-source",
    "ci": "yarn test && yarn lint && yarn flow check",
    "copy-source": "cp src/index.js lib/easy-ducks.cjs.js.flow && cp src/index.js lib/easy-ducks.esm.js.flow",
    "lint": "eslint src",
    "precommit": "yarn ci",
    "prepublish": "yarn build",
    "preversion": "yarn ci",
    "start": "webpack-dev-server --config example/webpack.config.js --port 7000",
    "test": "jest"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.67.1",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-fetch-mock": "^1.4.2",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-content-loader": "^3.1.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "style-loader": "^0.20.3",
    "styled-components": "^3.2.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./src/test/testHelper.js"
    ]
  },
  "keywords": [
    "async",
    "ducks",
    "react",
    "react-redux",
    "redux",
    "redux-thunk"
  ]
}
